        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="Users">
      <EntityTypeMapping TypeName="ST_LMSModel.User">
        <MappingFragment StoreEntitySet="Users">
